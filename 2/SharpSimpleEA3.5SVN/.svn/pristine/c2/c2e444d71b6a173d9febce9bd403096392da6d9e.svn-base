.Container
{
	  position: absolute;	
	  width: 100%;
	  height: 2px;
	  
}
#Scroller-1
{ 
	  position: absolute; 
	  overflow: hidden;
	  width: 100%;
	  height: 390px;
}


#Scroller-1 p 
{
	  font-family: Verdana, Arial, Helvetica, sans-serif;
	  font-size: 11px;
	  text-indent: 20px;
	  color: #6F6048;
}

#Scroller-Res
{ 
	  position: absolute; 
	  overflow: hidden;
	  width: 100%;
	  height: 390px;
}


#Scroller-Res p 
{
	  font-family: Verdana, Arial, Helvetica, sans-serif;
	  font-size: 11px;
	  text-indent: 20px;
	  color: #6F6048;
}

.Scroller-Container 
{
	  position:relative;
}

#Scrollbar-Container 
{
	  position: absolute;	
}

.Scrollbar-Track 
{
	  width: 20px;
	  height: 300px;
	  position: absolute;
	  top: 40px;
	  left: 12px;
	
}
.Scrollbar-Handle
{
	  position: absolute;
	  width: 13px;
	  height: 89px;	 
}


.Scrollbar-Up 
{
	  cursor:default;
	  position: absolute;	  
	  left:4px;
	  top:5px;
}

.Scrollbar-Down 
{
	  cursor:default;
	  position: absolute;
	  top: 350px;
	  left:4px;
}
.Scrollbar-Down1
{
	  cursor:default;
	  position: absolute;
	  top: 420px;
	  left:4px;
}

#Scroller-Set
{ 
	  position: absolute; 
	  overflow: hidden;
	  width: 93%;
	  height: 448px;
}

.Scrollbar-Down-Set
{
	  cursor:default;
	  position: absolute;
	  top: 410px;
	  left:4px;
}


.Scrollbar-Track-Set
{
	  width: 20px;
	  height: 365px;
	  position: absolute;
	  top: 40px;
	  left: 12px;
	
}

