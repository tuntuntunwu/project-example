.column
{
	width:98%;
	margin-right:.5%;
	margin-left:.5%;
	min-height:300px;
	background-color: transparent;	
	float:left;
}
.column .dragbox{
	margin:5px 2px  20px;	
	position:relative;
	background-image:url('../Images/Drakanddropbg.png');
	background-repeat:repeat-x;
	background-repeat-y:0%;
	
	BORDER-RIGHT: #596269 1px solid;     
    BORDER-LEFT: #596269 1px solid;
    BORDER-BOTTOM: #596269 1px solid;	
	
}
.column .dragbox h2
{
	margin:0;
	font-size:12px;
	padding:5px;
	height:25px;
	padding-left:25px;
	padding-top:8px;
	
	color:#000;	
	font-family:Verdana;
	cursor:move;
}
dragbox-content{
	background:#82bcdd;
	min-height:100px; 
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:10px;
	font-weight:normal;

}
.column  .placeholder{	
	border:1px dashed #ddd;
}
.dragbox h2.collapse
{	
	
}
.dragbox h2 .configure{
	font-size:11px;
	font-weight:normal;
	margin-right:30px;
	float:right;
}
