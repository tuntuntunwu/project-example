using System;

/// <summary>
/// Summary of LoginScreenType
/// </summary>
public class LoginScreenType
{
    private bool _bFixedScreen = true;
    private string _strMainMode = "COPY";
    private string _strSubMode = "DEFAULT";
    private bool _bLogoutScreen = false;
    private bool _bLogoutUseAllMFP = true;

    public LoginScreenType()
    {
    }

    public bool FixedScreen
    {
        get{return _bFixedScreen;}
        set{_bFixedScreen = value;}
    }
    public string MainMode
    {
        get { return _strMainMode; }
        set { _strMainMode = value; }
    }
    public string SubMode
    {
        get { return _strSubMode; }
        set { _strSubMode = value; }
    }
    public bool LogoutScreen
    {
        get { return _bLogoutScreen; }
        set { _bLogoutScreen = value; }
    }
    public bool LogoutUseAllMFP
    {
        get { return _bLogoutUseAllMFP; }
        set { _bLogoutUseAllMFP = value; }
    }

}
